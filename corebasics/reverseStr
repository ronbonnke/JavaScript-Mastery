function reverseString(str) {
  var reverse = "";
  for (var i = str.length - 1; i >= 0; i--) {
    reverse += str[i];
  }
  return reverse;
}
var originalName = "Ronbonnke";
var reversedName = reverseString(originalName);

console.log("orginal name =", originalName);
console.log("reversed name =", reversedName);
